{
  "name": "mcplay",
  "version": "1.0.2",
  "description": "A music palyer that counld be control by a H5 page online",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ mcplayer --win --out mcplayer --arch=x64 --version 1.3.4 --overwrite --ignore=node_modules",
    "pak" : "electron-packager . mcplayer --asar --prune --platform=win32 --arch=x64 --version=1.3.4 --overwrite --icon=views/favicon.ico --out=build",
    "pak32": "electron-packager . mcplayer --asar --prune --platform=win32 --arch=ia32 --version=1.4.3 --overwrite --icon=views/favicon.ico --out=build",
    "pakmas":"electron-packager ./ mcplayer --platform=mas --arch=x64 --version=1.4.6 --icon=views/favicon.icns  --out=build"
},
  "repository": "https://github.com/LanceCong/mcplayer",
  "keywords": [
    "Electron",
    "music",
    "player"
  ],
  "author": "Lance",
  "license": "MIT",
  "devDependencies": {
    "chokidar": "^1.5.1",
    "electron": "^9.1.0",
    "electron-packager": "^8.1.0",
    "mime-types": "^2.1.12",
    "express": "4.x.x",
    "body-parser": "*",
    "request":"*",
    "socket.io": "*"
    
  }
}
